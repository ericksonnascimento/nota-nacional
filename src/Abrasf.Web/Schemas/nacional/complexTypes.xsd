            <xsd:element name="Dps" type="TCDPS" minOccurs="1" maxOccurs="unbounded" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="Id" type="tsIdTag" />
    <xsd:attribute name="versao" type="TVerNFSe" use="required" />
