      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Schemas\consultarUrlNfse.xsd">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Schemas\enviarLoteRps.xsd">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
